# Ignore vendor directories
/vendor/

# Ignore node_modules directory
/node_modules/

# Ignore PHPStorm and IntelliJ IDEA files
/.idea/
/.phpstorm/

# Ignore PHPUnit cache
/phpunit.cache/

# Ignore PHP Code Coverage cache
/.php-code-coverage/

# Ignore Composer lock file
/composer.lock

# Ignore yarn and npm lock files
/yarn.lock
/package-lock.json

# Ignore various log files
*.log
*.tmp
*.pid

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore IDE-specific files
.vscode/

/runtime/*
!/runtime/cache/
!/runtime/cache/.gitkeep
!/runtime/debug/
!/runtime/debug/.gitkeep
!/runtime/logs/
!/runtime/logs/.gitkeep

# Ignore everything in web/assets, but allow .gitkeep
/web/assets/*
!/web/assets/.gitkeep

# Ignore everything in web/uploads, but allow .gitkeep
/web/uploads/*
!/web/uploads/.gitkeep

# Ignore everything in web/protected, but allow .gitkeep
/web/protected/*
!/web/protected/.gitkeep

# Ignore test output files
/tests/_output/






